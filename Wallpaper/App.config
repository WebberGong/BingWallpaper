<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
        <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
    <appSettings>
        <!--自动下载壁纸时间间隔（单位：小时）-->
        <add key="AutoDownloadWallpaperInterval" value="1" />
        <!--自动更换壁纸时间间隔（单位：分钟）-->
        <add key="AutoChangeWallpaperInterval" value="5" />
    </appSettings>
</configuration>